class Bank_v1:
    bank_name='SBI'
    bank_ifsc='sbin00092'
    bank_roi=6
    bank_branch='hyderbad'
    def __init__(self,na,a,ac,cb):
        self.cname=na
        self.cage=a
        self.caccount=ac
        self.cbal=cb
    @staticmethod
    def get_int_value():
        integer=int(input('enter integer value:'))
        return integer
    
