### class###

class Bank:
    bank_name='sbi'
    bank_roi=5
    bank_branch='kizikistan' 
ankit=Bank()# WE ARE CALLING OBJECT
rohit=Bank()# WE ARE CALLING OBJECT



#### generic method###   


# # ACESSING GENERIC PROPERTIES BY USING CLASS
     ## className.classvariablename
print(Bank.bank_name)
print(Bank.bank_name)



# # ACESSING GENERIC PROPERTIES BY USING OBJECT
     # OBJECTName.classvariablename
print(ankit.bank_name)
print(rohit.bank_name)




#   #   # MODIFYING GENERIC PROPERTIES BY USING CLASS
    # className.classVariableName=New Value

Bank.bank_roi=4
print(Bank.bank_name)
print(ankit.bank_roi)
print(rohit.bank_roi)
