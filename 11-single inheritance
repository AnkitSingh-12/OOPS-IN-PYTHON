class Parent:
    house='2bhk'
    bike='chetak'
class Child(Parent):
    bike='KTM'
    money=20000

PCO=Parent()
CCO=Child()

# modiyfying properties by using parent class 

Parent.house='1bhk'
print(Parent.house)
print(PCO.house)
print(Child.house)
print(CCO.house)


#parent class and parent class object and child class object
#child class object will get effected
#modify property by using parent class object
#only that particular object will get affected 


PCO.house='1BHK'
print(Parent.house)
print(PCO.house)
print(Child.house)
print(CCO.house)
